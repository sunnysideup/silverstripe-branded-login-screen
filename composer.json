{
  "name": "sunnysideup/branded-login-screen",
  "description": "Provides you with the option to brand your login screen.",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "login",
    "branding",
    "brand",
    "logo",
    "personalise",
    "log-in",
    "multi-factor",
    "various log-in options"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0",
    "tractorcow/silverstripe-colorpicker": "^4"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\BrandedLoginScreen\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/sake-npm-build-branded-login-screen.sh",
    "bin/sake-npm-watch-branded-login-screen.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
